{
    "name": "hobiecat/italiagov-custom-installer",
    "type": "composer-plugin",
    "license": "MIT",
    "description": "Custom installer for the albosegretari site",
    "keywords": [
        "installer"
    ],
    "authors": [
        {
            "name": "Giorgio Consorti",
            "email": "g.consorti@lynxlab.com"
        }
    ],
    "autoload": {
        "psr-4": { "Lynxlab\\Installers\\": "src/Lynxlab/Installers" }
    },
    "extra": {
        "class": "Lynxlab\\Installers\\Plugin",
        "plugin-modifies-install-path": true
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "composer-plugin-api": "^1.0 || ^2.0"
    }
}
